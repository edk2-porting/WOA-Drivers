<ConstraintModel>
  
  <Limits> 
    <Resource>
      <ID>AudioSession</ID>
      <Consumption>8</Consumption>
    </Resource>
    <Resource>
      <ID>OffloadRestrictions</ID>
      <Consumption>2</Consumption> <!-- Global limitation on OffloadRestrictions streams -->
    </Resource>	
  </Limits>
 
  <ResourceConsumer>
    <!-- Active Phone call -->
